{
  "hosting": {
    "source": ".",
    "public": "out",
    "headers": [
      {
        "source": "**/*.@(png|jpg|jpeg|gif|webp|avif|svg|ico)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "**/*.@(woff|woff2|ttf|otf)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/sitemap.xml",
        "destination": "https://storage.googleapis.com/real-motor-japan.firebasestorage.app/sitemap.xml"
      }
    ],
    "redirects": [
      {
        "source": "/sitemaps/:path*",
        "destination": "https://storage.googleapis.com/real-motor-japan.firebasestorage.app/sitemaps/:path*",
        "type": 301
      }
    ]
  }
}